	local function shuffleServers()
		local teleportFunc = queueonteleport or queue_on_teleport

		if teleportFunc then
			teleportFunc([[
        if not game:IsLoaded() then
            game.Loaded:Wait()
        end
        repeat wait() until game.Players.LocalPlayer
        
        local data = game:GetService("TeleportService"):GetLocalPlayerTeleportData()
        
        local teleportFunc = queueonteleport or queue_on_teleport
        
        if teleportFunc then
        	teleportFunc("loadstring(game:HttpGet("https://raw.githubusercontent.com/VJerebliuc/Slap-battles-scripts/refs/heads/main/ultrafarm"))();_G.continuefarming = true;_G.tpdata = data")
        game:GetService("TeleportService"):Teleport(9015014224)
    	]])
		end
		
		local data = {
			[1] = farming,
			[2] = sfarming,
			[3] = antirecording,
			[4] = antiafkkicking,
			[5] = antikicking
		}

		game:GetService("TeleportService"):Teleport(11520107397,lplr,data)
	end

	local function grabSlapples()
		for i, v in ipairs(workspace.Arena.island5.Slapples:GetDescendants()) do
			if lplr.Character:FindFirstChild("HumanoidRootPart") and lplr.Character:FindFirstChild("entered") and v.Name == "Glove" and v:FindFirstChildWhichIsA("TouchTransmitter") then
				firetouchinterest(lplr.Character.HumanoidRootPart, v, 0)
				firetouchinterest(lplr.Character.HumanoidRootPart, v, 1)
			end
		end
	end
	
	---------
	
	local function farmFunc()
		if lplr.leaderstats.Glove.Value == "Replica" and lplr.Character.IsInDefaultArena.Value == true then
			lplr.Character:PivotTo(spot.CFrame + Vector3.new(0,100,0))

			wait(1)

			spawn(function()
				while wait(0.5) and farming do
					for _,v in pairs(game.Players:GetPlayers()) do
						if v:GetRankInGroup(9950771) >= 2 then
							lplr:Kick("HIGH STATUS PLAYER DETECTED! (TESTER+): "..v.DisplayName.." ("..v.Name..")")
						end
					end
				end
			end)

			spawn(function()
				while farming do
					game.ReplicatedStorage.Duplicate:FireServer()
					wait(19.9)
				end
			end)

			while farming do
				for i, v in pairs(workspace:GetChildren()) do
					if v.Name:match(lplr.Name) and v:FindFirstChild("HumanoidRootPart") then
						game.ReplicatedStorage.b:FireServer(v:WaitForChild("HumanoidRootPart"))
					end
				end
				task.wait()
			end
		end
	end
	
	local function antirecordFunc()
		spawn(function()
			while antirecording do
				game.Players.PlayerAdded:Connect(function(plr)

					plr.Chatted:Connect(function(msg)
						local words = msg:split(" ")

						if antirecord then
							for _,word in pairs(words) do
								for _,badword in pairs(badWords) do
									if word:lower():find(badword) then
										shuffleServers()
										lplr:Kick("POSSIBLE RECORDING DETECTED: ( ["..plr.DisplayName.."/"..plr.Name.."]: "..msg.." )")
									end
								end
							end
						end
					end)
				end)

				for _,v in ipairs(game.Players:GetPlayers()) do

					v.Chatted:Connect(function(msg)
						local words = msg:split(" ")

						if antirecord then
							for _,word in pairs(words) do
								for _,badword in pairs(badWords) do
									if word:lower():find(badword) then
										shuffleServers()
										lplr:Kick("POSSIBLE RECORDING DETECTED: ( ["..v.DisplayName.."/"..v.Name.."]: "..msg.." )")
									end
								end
							end
						end
					end)
				end

				wait(1)
			end
		end)
	end

	ping.MouseButton1Click:Connect(function()
		spawn(function()
			ping.Text = "PONG!"
			wait(0.5)
			ping.Text = "PING!"
		end)

		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "PONG!", Text = "you pinged the script"})
	end)

	farm.MouseButton1Click:Connect(function()
		farming = not farming

		if farming then
			farm.Text = "NO FARM"

			farmFunc()
		else
			farm.Text = "FARM"
		end
	end)

	autotycoon.MouseButton1Click:Connect(function()
		sfarming = not sfarming

		if sfarming then
			autotycoon.Text = "NO SLAPPLES"

			while sfarming do
				grabSlapples()
				task.wait()
			end
		else
			autotycoon.Text = "SLAPPLES"
		end
	end)

	grabslapples.MouseButton1Click:Connect(grabSlapples)

	antirecord.MouseButton1Click:Connect(function()
		antirecording = not antirecording

		if antirecording then
			antirecord.Text = "NO ANTI RECORD"

			antirecordFunc()
		else
			antirecord.Text = "ANTI RECORD"
		end
	end)

	equip.MouseButton1Click:Connect(function()
		if not lplr.Character:FindFirstChild("entered") then
			if workspace:FindFirstChild("Lobby") and workspace.Lobby:FindFirstChild("Replica") then
				fireclickdetector(workspace.Lobby.Replica.ClickDetector)
			end
		end
	end)

	antiafkkick.MouseButton1Click:Connect(function()
		antiafkkicking = not antiafkkicking

		if antiafkkicking then
			antiafkkick.Text = "NO ANTI AFK KICK"

			afkconnetion = lplr.Idled:Connect(function()
				virtualUser:CaptureController()
				virtualUser:ClickButton2(Vector2.new())
			end)
		else
			antiafkkick.Text = "ANTI AFK KICK"

			if afkconnetion then afkconnetion:Disconnect() end
		end
	end)

	antikick.MouseButton1Click:Connect(function()
		antikicking = not antikicking

		if antikicking then
			antikick.Text = "NO ANTI KICK"

			while antikicking do
				if #game.CoreGui.RobloxPromptGui.promptOverlay:GetChildren() > 0 then
					shuffleServers()
				end

				task.wait(1)
			end
		else
			antikick.Text = "ANTI KICK"
		end
	end)

	tptodefault.MouseButton1Click:Connect(function()
		if not lplr.Character:FindFirstChild("entered") then
			--if workspace:FindFirstChild("Lobby") and workspace.Lobby:FindFirstChild("Teleport2") then
			lplr.Character:PivotTo(workspace.Lobby.Teleport2.CFrame)
			--end
		end
	end)

	destroygui.MouseButton1Click:Connect(function()
		farming = false
		sfarming = false
		antirecording = false
		antiafkkicking = false
		antikicking = false

		if afkconnetion then afkconnetion:Disconnect() end

		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "GOODBYE!", Text = "goodbye friend! ;)"})

		screengui:Destroy()
	end)

	close.MouseButton1Click:Connect(function()
		gui.Visible = false
		open.Visible = true
	end)

	open.MouseButton1Click:Connect(function()
		gui.Visible = true
		open.Visible = false
	end)
	
	if resume then
		repeat
			wait()
		until game:IsLoaded()
		
		for i = 1,#data do
			if i == 1 then farming = data[i] end
			if i == 2 then sfarming = data[i] end
			if i == 3 then antirecording = data[i] end
			if i == 4 then antiafkkicking = data[i] end
			if i == 5 then antikicking = data[i] end
		end
		
		if farming then
			if workspace:FindFirstChild("Lobby") and workspace.Lobby:FindFirstChild("Replica") then
				fireclickdetector(workspace.Lobby.Replica.ClickDetector)
			end
			
			lplr.Character:PivotTo(workspace.Lobby.Teleport2.CFrame)
			
			farm.Text = "NO FARM"

			farmFunc()
		end
		if sfarming then
			autotycoon.Text = "NO SLAPPLES"

			while sfarming do
				grabSlapples()
				task.wait()
			end
		end
		if antirecording then
			antirecord.Text = "NO ANTI RECORD"

			antirecordFunc()
		end
		if antiafkkicking then
			antiafkkick.Text = "NO ANTI AFK KICK"

			afkconnetion = lplr.Idled:Connect(function()
				virtualUser:CaptureController()
				virtualUser:ClickButton2(Vector2.new())
			end)
		end
		if antikicking then
			antikick.Text = "NO ANTI KICK"

			while antikicking do
				if #game.CoreGui.RobloxPromptGui.promptOverlay:GetChildren() > 0 then
					shuffleServers()
				end

				task.wait(1)
			end
		end
	end
end

if _G.continuefarming == true then
	local transferredData = _G.tpdata
	
	main(true,transferredData)
end

