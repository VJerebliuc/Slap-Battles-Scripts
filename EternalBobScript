extrarob.Name = "extrarob"
extrarob.Size = UDim2.new(0.15,0,0.3,0)
extrarob.Position = UDim2.new(0.75,0,0.1,0)
extrarob.Text = "ATTEMPT TO USE ROB"
extrarob.TextScaled = true
